<Window x:Class="LibraryClient.AddBookDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LibraryClient"
        mc:Ignorable="d"
        Title="AddBookDialog" Height="231" Width="308.333">
    <Grid>
        <TextBox x:Name="ISBNEntryTextBox" HorizontalAlignment="Left" Height="23" Margin="40,145,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="220" GotFocus="ISBNEntryTextBox_GotFocus" KeyDown="ISBNEntryTextBox_KeyDown"/>
        <Button x:Name="YesButon" Content="Yes" HorizontalAlignment="Left" Margin="165,120,0,0" VerticalAlignment="Top" Width="95" Click="YesButon_Click"/>
        <Button x:Name="NoButton" Content="No" HorizontalAlignment="Left" Margin="40,120,0,0" VerticalAlignment="Top" Width="90" Click="NoButton_Click"/>
        <Label x:Name="QuestionLabel" Content="Label" HorizontalContentAlignment="Center" HorizontalAlignment="Left" Margin="40,25,0,0" VerticalAlignment="Top" Height="90" Width="220"/>

    </Grid>
</Window>
